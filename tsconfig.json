{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "paths": {
      "lib-ui-charts": [
        "dist/lib-ui-charts/lib-ui-charts",
        "dist/lib-ui-charts"
      ],
      "lib-ui-components": [
        "dist/lib-ui-components/lib-ui-components",
        "dist/lib-ui-components"
      ]
    },
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom",
      "esnext.asynciterable"
    ],
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  }
}
